{
	"name": "@ylide/social-hub",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"start": "react-app-rewired start",
		"start-mainview": "REACT_APP__APP_MODE=MAIN_VIEW react-app-rewired start",
		"start-hub": "REACT_APP__APP_MODE=HUB react-app-rewired start",
		"start-otc": "REACT_APP__APP_MODE=OTC REACT_APP__OTC_PROVIDER=AIRSWAP react-app-rewired start",
		"build-hub": "react-app-rewired build",
		"build-mainview": "REACT_APP__APP_MODE=MAIN_VIEW react-app-rewired build",
		"build-airswap": "REACT_APP__APP_MODE=OTC REACT_APP__OTC_PROVIDER=AIRSWAP react-app-rewired build",
		"build-paraswap": "REACT_APP__APP_MODE=OTC REACT_APP__OTC_PROVIDER=PARASWAP react-app-rewired build",
		"build-1inch": "REACT_APP__APP_MODE=OTC REACT_APP__OTC_PROVIDER=ONEINCH react-app-rewired build",
		"check": "tsc && eslint --max-warnings 0 . && prettier --list-different .",
		"test": "react-app-rewired test",
		"eject": "react-app-rewired eject"
	},
	"eslintConfig": {
		"root": true,
		"extends": [
			"react-app",
			"react-app/jest"
		],
		"plugins": [
			"simple-import-sort"
		],
		"rules": {
			"no-restricted-globals": "off",
			"simple-import-sort/imports": "warn",
			"simple-import-sort/exports": "warn"
		}
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
