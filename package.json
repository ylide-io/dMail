{
	"name": "@ylide/social-hub",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"mainview": "npm run build && npm run start-local",
		"start": "cross-env REACT_APP__APP_MODE=MAIN_VIEW react-app-rewired start",
		"build": "cross-env REACT_APP__APP_MODE=MAIN_VIEW REACT_APP__CIRCLE_SHA1=$CIRCLE_SHA1 REACT_APP__CIRCLE_BUILD_NUM=$CIRCLE_BUILD_NUM react-app-rewired build",
		"start-local": "http-server ./build -S -C cert.pem -o -a localhost -p 8080",
		"check": "tsc && eslint --max-warnings 0 . && prettier --list-different .",
		"test": "react-app-rewired test",
		"eject": "react-app-rewired eject"
	},
	"eslintConfig": {
		"root": true,
		"extends": [
			"react-app",
			"react-app/jest"
		],
		"plugins": [
			"simple-import-sort"
		],
		"rules": {
			"no-restricted-globals": "off",
			"simple-import-sort/imports": "warn",
			"simple-import-sort/exports": "warn"
		}
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"dependencies": {
		"@jitsu/sdk-js": "3.1.5",
		"@react-hook/resize-observer": "1.2.6",
		"@sentry/react": "7.57.0",
		"@web3modal/wagmi": "^3.5.2",
		"clsx": "1.2.1",
		"ethereum-blockies-base64": "1.0.2",
		"jest": "29.5.0",
		"lodash.difference": "^4.5.0",
		"lodash.sortby": "^4.7.0",
		"minireset.css": "0.0.7",
		"mobx": "6.9.0",
		"mobx-react": "7.6.0",
		"nanoid": "4.0.2",
		"normalize.css": "8.0.1",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"react-helmet": "^6.1.0",
		"react-image-gallery": "1.2.12",
		"react-intersection-observer": "9.5.1",
		"react-query": "3.39.3",
		"react-router-dom": "6.14.1",
		"react-share": "4.4.1",
		"react-svg": "16.1.6",
		"react-virtualized-auto-sizer": "1.0.20",
		"react-window": "1.8.9",
		"sass": "1.63.6",
		"typescript": "5.1.3",
		"ua-parser-js": "^1.0.36",
		"workbox-background-sync": "^6.6.0",
		"workbox-broadcast-update": "^6.6.0",
		"workbox-cacheable-response": "^6.6.0",
		"workbox-core": "^6.6.0",
		"workbox-expiration": "^6.6.0",
		"workbox-google-analytics": "^6.6.0",
		"workbox-navigation-preload": "^6.6.0",
		"workbox-precaching": "^6.6.0",
		"workbox-range-requests": "^6.6.0",
		"workbox-routing": "^6.6.0",
		"workbox-strategies": "^6.6.0",
		"workbox-streams": "^6.6.0",
		"viem": "^1.21.1",
		"wagmi": "^1.4.12",
		"ws": "^8.14.1"
	},
	"devDependencies": {
		"@types/lodash.difference": "^4.5.7",
		"@types/lodash.sortby": "^4.7.7",
		"@types/react-helmet": "^6.1.6",
		"@types/ua-parser-js": "^0.7.37",
		"@types/ws": "^8.5.5",
		"@types/jest": "29.5.2",
		"@types/react": "18.2.14",
		"@types/react-dom": "18.2.6",
		"@types/react-virtualized-auto-sizer": "1.0.1",
		"@types/react-window": "1.8.5",
		"assert": "2.0.0",
		"buffer": "6.0.3",
		"cross-env": "^7.0.3",
		"crypto-browserify": "3.12.0",
		"eslint": "8.43.0",
		"eslint-plugin-simple-import-sort": "10.0.0",
		"https-browserify": "1.0.0",
		"os-browserify": "0.3.0",
		"prettier": "2.8.8",
		"process": "0.11.10",
		"react-app-rewired": "2.2.1",
		"react-scripts": "5.0.1",
		"sass-loader": "13.3.2",
		"source-map-loader": "4.0.1",
		"stream-browserify": "3.0.0",
		"stream-http": "3.2.0",
		"url": "0.11.1",
		"webpack-bundle-analyzer": "4.9.0"
	}
}
